{
    "name": "demo-geolocation",
    "version": "0.0.0",
    "description": "Multi-user geolocation prototype",
    "license": "MIT",
    "keywords": [],
    "scripts": {
        "build:es": "webpack",
        "watch:es": "watch 'npm run build:es' ./src",
        "lint": "eslint . -c ./.eslintrc.js --fix",
        "lint:ing": "watch 'npm run lint' ./src",
        "lint:tests": "eslint __tests__/**/*.js -c ./.eslintrc.js --fix --no-ignore",
        "build:css": "postcss ./assets/css/style.css --dir ./dist",
        "watch:css": "npm run build:css -- --watch",
        "test": "jest .*.test.js --coverage",
        "test:ing": "npm test -- --watchAll",
        "predeploy": "npm-run-all clean \"build:es -- --mode=production\" build:css",
        "deploy": "surge dist",
        "clean": "del-cli ./dist",
        "copy": "npm-run-all --parallel copy:assets copy:index",
        "copy:assets": "cpy './assets/!(css)/**/*.*' './assets/**/[.]*' ./dist --parents --recursive",
        "copy:index": "cpy './assets/index.html' ./dist",
        "prebuild:es": "del-cli dist/assets",
        "postbuild:es": "npm run copy",
        "watch:assets": "watch 'npm run copy' ./assets",
        "dashboard": "webpack-dashboard -- webpack-dev-server --config ./webpack.config.js",
        "dev": "stmux [ \"npm run test:ing\" : \"npm run lint:ing\" ]",
        "prestart": "npm run build:es",
        "start": "npm-run-all --parallel watch:es watch:css serve",
        "serve": "browser-sync start --server ./dist --files ./dist --port 4669"
    },
    "devDependencies": {
        "@babel/cli": "^7.7.0",
        "@babel/core": "^7.7.2",
        "@babel/plugin-proposal-class-properties": "^7.7.0",
        "@babel/plugin-proposal-export-default-from": "^7.5.2",
        "@babel/plugin-proposal-optional-chaining": "^7.6.0",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@babel/preset-env": "^7.7.1",
        "@babel/runtime": "^7.7.2",
        "babel-eslint": "^10.0.3",
        "babel-jest": "^24.9.0",
        "babel-loader": "^8.0.6",
        "babel-preset-minify": "^0.5.1",
        "browser-sync": "^2.26.7",
        "cpy-cli": "^2.0.0",
        "cssnano": "^4.1.10",
        "del-cli": "^3.0.0",
        "eslint": "^6.6.0",
        "eslint-config-omaha-prime-grade": "^11.0.4",
        "eslint-plugin-lit": "^1.2.0",
        "jest": "^24.9.0",
        "jest-watch-typeahead": "^0.4.0",
        "npm-run-all": "^4.1.5",
        "postcss-cli": "^6.1.3",
        "postcss-import": "^12.0.1",
        "postcss-preset-env": "^6.7.0",
        "postcss-reporter": "^6.0.1",
        "postcss-safe-parser": "^4.0.1",
        "stmux": "^1.7.1",
        "stylelint": "^11.1.1",
        "stylelint-config-recommended": "^3.0.0",
        "terser-webpack-plugin": "^2.2.1",
        "watch": "^1.0.2",
        "webpack": "^4.41.2",
        "webpack-cli": "^3.3.10",
        "webpack-dashboard": "^3.2.0",
        "webpack-dev-server": "^3.9.0",
        "webpack-jarvis": "^0.3.2"
    },
    "jest": {
        "testMatch": [
            "**/__tests__/**/*.(e2e|test).[jt]s?(x)"
        ],
        "setupFilesAfterEnv": [
            "<rootDir>/__tests__/setup.js"
        ],
        "watchPlugins": [
            "jest-watch-typeahead/filename",
            "jest-watch-typeahead/testname"
        ]
    },
    "dependencies": {
        "backbone": "^1.4.0",
        "backbone.marionette": "^4.1.2",
        "backbone.radio": "^2.0.0",
        "cesium": "^1.63.1",
        "copy-webpack-plugin": "^5.0.5",
        "css-loader": "^3.2.0",
        "jquery": "^3.4.1",
        "lit-html": "^1.1.2",
        "lodash-es": "^4.17.15",
        "marionette.approuter": "^1.0.2",
        "ramda": "^0.26.1",
        "redux": "^4.0.4",
        "style-loader": "^1.0.0",
        "url-loader": "^2.2.0"
    },
    "main": "./src/main.js"
}
